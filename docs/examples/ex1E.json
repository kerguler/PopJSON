{
    "model": {
        "title": "Climate-sensitive population dynamics of Aedes albopictus",
        "type": "Population",
        "url": "https://github.com/kerguler/Population",
        "deterministic": true,
        "parameters": {
            "algorithm": "Population",
            "istep": 0.01
        }
    },
    "environ": [
        {
            "id": "temp",
            "name": "Temperature of the breeding pool (in Â°C)",
            "url": ""
        }
    ],
    "functions": {
        "briere1": ["define", ["T","a","L","R"], ["?",["<=","T","L"],"365.0", ["?",[">=","T","R"],"365.0", ["min","365.0",["max","1.0", ["/","1.0",["exp", ["+","a",["log","T"],["log",["-","T","L"]],["*","0.5",["log",["-","R","T"]]]]]]]]]]]
    },
    "intermediates": [
        {
            "id": "d2m",
            "value": ["briere1", ["index","temp","TIME_1"], "d2m_a", "d2m_b", "d2m_c"]
        }, {
            "id": "d2s",
            "value": ["*", "d2s_c", "d2m"]
        }
    ],
    "parameters": [
        {
            "id": "d2m_a",
            "constant": false,
            "name": "Larva development mean (a)",
            "value": -12
        }, {
            "id": "d2m_b",
            "constant": false,
            "name": "Larva development mean (b)",
            "value": 283.15
        }, {
            "id": "d2m_c",
            "constant": false,
            "name": "Larva development mean (c)",
            "value": 308.15
        }, {
            "id": "d2s_c",
            "constant": false,
            "name": "Larva development stdev (c)",
            "value": 0.2
        }
    ],
    "populations": [
        {
            "id": "larva",
            "name": "The larva stage",
            "processes": [
                {
                    "id": "larva_dev",
                    "name": "Larva development time",
                    "arbiter": "ACC_ERLANG",
                    "value": ["d2m", "d2s"]
                }
            ]
        }
    ]
}
